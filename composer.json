{
    "name": "christyoga123/db-structure-viewer",
    "description": "Laravel package to view database structure and related migrations",
    "type": "library",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Christyoga123\\DbStructureViewer\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Christyoga123\\DbStructureViewer\\DbStructureViewerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}